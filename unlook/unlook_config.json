{
    "server": {
        "name": "UnLookScanner",
        "control_port": 5555,
        "stream_port": 5556,
        "scanner_uuid": null,
        "log_level": "INFO",
        "log_file": "/var/log/unlook/server.log"
    },
    "projector": {
        "i2c_bus": 3,
        "i2c_address": "0x1B",
        "standby_on_start": true,
        "test_on_start": false
    },
    "camera": {
        "default_resolution": [1920, 1080],
        "default_fps": 30
    },
    "gpio": {
        "status_led_pin": 25,
        "error_led_pin": 24,
        "reset_button_pin": 23,
        "use_gpio": true
    }
}